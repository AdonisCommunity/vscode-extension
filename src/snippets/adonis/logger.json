{
    "Logger": {
        "body": [
            "${1:Logger}.${2|emerg,alert,crit,error,warning,notice,info,debug|}(${3:msg}, ${4:...data})"
        ],
        "description": "Adonis Logger",
        "prefix": "log"
    },
    "Logger Debug": {
